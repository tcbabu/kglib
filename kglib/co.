int kgListRemoveDup_o(void *Wid) {
	Dlink *L=NULL;
	ThumbNail **TH,*th;
	void *pt;
	int i,n;
	TH = (ThumbNail **)kgGetList(Wid);
	if (TH == NULL) return 0;
	L = Dopen();
	i=0;
	while(TH[i] != NULL) {
		Dadd(L,TH[i]);
		i++;
	}
	n = Dcount(L)+1;
	if(n==1) return 0;
	Resetlink(L);
	printf ("Count 1: %d\n",n);
	Drmvdup_cond(L,uiDupItem);
	printf ("Count 2: %d\n",Dcount(L)+1);
	Resetlink(L);
	i=0;
	while( (pt=Getrecord(L))!= NULL) {TH[i]=pt;i++;}
	TH[i]=NULL;
	kgSetList(Wid,(void **)TH);
	Dfree(L);
	return 1;
}
